<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="DefaultHeader">
        <attr name="title" format="string" />
        <attr name="titleSize" format="dimension" />
        <attr name="titleStyle" format="integer">
            <enum name="normal" value="0" />
            <enum name="bold" value="1" />
            <enum name="italic" value="2" />
        </attr>
        <attr name="backgroundColor" format="color" />
        <attr name="titleTextColor" format="color" />
        <attr name="backButtonColor" format="color" />
        <attr name="backButtonEnabled" format="boolean" />
        <attr name="optionButtonEnabled" format="boolean"/>
        <attr name="backButtonImageSource" format="reference|integer" />
        <attr name="optionButtonOutline" format="boolean" />
        <attr name="optionButtonImageSource" format="reference|integer" />
        <attr name="optionButtonText" format="reference|string" />
    </declare-styleable>

    <declare-styleable name="TextDivider">
    <attr name="title2" format="string" />
    </declare-styleable>
</resources>