                        android:background="@color/grey_EC" />


                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            style="@style/position_data_TEXTVIEW_STYLE1"
                            android:text="기준국 전원" />

                        <View
